{
  "name": "cap-powershell",
  "version": "2.0.7",
  "description": "Plugin PowerShell Capacitor",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "build-electron": "rimraf ./electron/dist && cd ./electron && tsc && rollup --config rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Herv√© de CHAVIGNY",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest",
    "fs": "0.0.1-security",
    "fs-jetpack": "^3.1.0",
    "node-notifier": "^8.0.0",
    "node-powershell": "^4.0.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "ssh2": "^0.8.9",
    "systeminformation": "^4.26.10"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "@types/node": "^12.0.0",
    "electron": "^18.3.7",
    "rimraf": "^3.0.0",
    "rollup": "^2.18.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslib": "^1.13.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "electron/",
    "CapPowershell.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    },
    "electron": {
      "src": "electron/dist/plugin.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/vevedh/cap-powershell.git"
  },
  "bugs": {
    "url": "http://github.com/vevedh/cap-powershell.git/issues"
  }
}
